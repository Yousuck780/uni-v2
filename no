local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Ski hub universal loader", "Ocean")
local Tab = Window:NewTab("Home")
local Section = Tab:NewSection("Loader")
Section:NewButton("Load ski hub universal", "Load the script!", function()
	loadstring(game:HttpGet('https://raw.githubusercontent.com/Yousuck780/uni/main/uni'))()
end)
Section:NewLabel("If you joined a game that supports ski hub")
Section:NewLabel("Close this gui!")
